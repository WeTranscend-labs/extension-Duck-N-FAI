{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with React, TypeScript and Tailwind",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_title": "React Chrome Extension"
  },
  "icons": {
    "16": "./icons/rubber-duck.png",
    "48": "./icons/rubber-duck.png",
    "128": "./icons/rubber-duck.png"
  }
}
